<div id="dialog-form" title="Promote Member">
  <% @members.each do |m| %>
  <table class="table table-striped">    
    <% if m.user_id != current_user.id %>        
    <tr>
      <td>
      <%= m.user.name %> 
      </td>         
      <td>        
        <% if m.isCreator == true %>
        <%= link_to("creator",{:action => 'promoteMembers', :group_id=> @group.id,   :user_id => m.user_id}, :class =>'btn btn-primary', :confirm => "هل تريد جعله ناشئ للمجموعة؟") %>
      </td>
      <td>
        <% else %>     
          <%= link_to("not creator",{:action => 'promoteMembers', :group_id=> @group.id, :user_id => m.user_id}, :class =>'btn btn-primary', :confirm => "هل تريد جعلهو عضو عادي؟") %>
      </td>
        <% end %>
        <% end %>
    </tr> <br>
     <% end %>
  </table>
</div> 

