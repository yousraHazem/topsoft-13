<meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
<head dir="rtl">
 <title> My Project List </title>
</head>

<body dir="rtl" lang="ar" class="body">
<h3>ماشروعاتي</h3>
<% if @projectusers.blank? %>
	<p>ليس لديك أي مشروعات حاليا</p>
<% end %>
<table>
  <tr>
  </tr>

<% @projectusers.each do |project_user| %>
  <tr>
    <td id ="project_name"><%= link_to(project_user.project.name, { :controller => 'projects',:action=>'show', :id=> project_user.project_id} , :class=> "link" ) %></td>
  </tr>
<% end %>
</table>
</body>