<% @notifications.each do |t|%>
<% url = t.notification.url %>
<a href="<%= url %>" style="text-decoration: none;">
<div class="<%= t.read == false ? 'notification-list unread' : 'notification-list read' %>"  data-time="<%= t.created_at.to_i %>"> 
<%= t.notification.content %><i class="date pull-left" dir="ltr"><%= time_ago_in_words(t.created_at)%><i> ago </i></i><br><br><hr>
</div> 
</a> 
<%end%>
